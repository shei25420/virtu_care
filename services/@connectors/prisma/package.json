{
  "name": "@connectors/prisma",
  "version": "0.0.0",
  "description": "Prisma Database Connector Package",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "schema:clean; prisma generate --schema ./prisma/schema.prisma",
    "schema:clean": "rm -rf ./prisma",
    "schema:init": "prisma init",
    "schema:merge": "cat ./models/* >> ./prisma/schema.prisma",
    "schema:push": "prisma db push --schema ./src/plugins/prisma/schema.prisma",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "prisma": "^5.3.1"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1"
  }
}
